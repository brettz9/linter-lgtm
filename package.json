  },
  "license": "MIT",
  "repository": "https://github.com/brettz9/linter-lgtm",
  "homepage": "https://github.com/brettz9/linter-lgtm",
  "bugs": "https://github.com/brettz9/linter-lgtm/issues",
  "configSchema": {
